{
    "name": "foxbyte/smart-link",
    "description": "A PHP package for generating smart links.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Komang Krisnanda",
            "email": "krisnanda@fgi.co.id"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
